  "main": "index.js",
  "scripts": {
    "build": "npm -prefix client run build",
    "preinstall": "cd ./server && npm install",
    "install": "cd ./client && npm install",
    "start": "npx concurrently 'npm run start:server' 'npm run start:client'",
    "start:backend": "npm --prefix ./server start",
    "start:client": "npm --prefix ./client start",
    "start:dev": "npx concurrently 'npm run start:dev --prefix ./server' 'npm start --prefix ./client'",
    "test": "npm run test:client && npm run test:backend"
  },
